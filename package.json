{
  "name": "@marinda/react-styled-buttons",
  "version": "0.0.10",
  "description": "React button components with WCAG 2 AA contrast ratio tested colors, light and dark theme options. Flexible & easily customized.",
  "repository": "mariiinda/react-styled-buttons",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .cache/ dist/",
    "prestart": "npm run clean",
    "start": "parcel src/index.html",
    "build": "babel ./src --out-dir ./dist -s inline",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "sketch-build": "skpm-build",
    "sketch-render": "skpm-build --watch --run",
    "sketch-render:once": "skpm-build --run",
    "sketch-link": "npm run build && skpm-link"
  },
  "author": "marinda s",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@skpm/builder": "^0.7.2",
    "@storybook/addon-a11y": "^5.2.0-beta.20",
    "@storybook/addon-actions": "^5.2.0-beta.20",
    "@storybook/addon-knobs": "^5.2.0-beta.20",
    "@storybook/addon-links": "^5.2.0-beta.20",
    "@storybook/addons": "^5.2.0-beta.20",
    "@storybook/cli": "^5.2.0-beta.18",
    "@storybook/react": "^5.2.0-beta.20",
    "@testing-library/dom": "^5.6.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "react-sketchapp": "^3.0.0-beta.9",
    "react-test-renderer": "^16.8.6",
    "webpack": "^4.37.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "@emotion/core": "^10.0.14"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "emotion-theming": "^10.0.14",
    "open-color": "^1.7.0"
  },
  "prettier": {},
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "react-styled-buttons",
    "manifest": "sketch/src/manifest.json",
    "main": "sketch/react-styled-buttons.sketchplugin"
  }
}
